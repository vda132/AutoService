<Window x:Class="WpfApp1.Manipulation"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        xmlns:Control="clr-namespace:WpfApp1.UserControllers"
        mc:Ignorable="d"
        Title="Manipulation" Height="450" Width="800">
    <Grid>
        <Control:DBAdministratorController x:Name="DBAdministratorController" Visibility="Hidden"/>
        <Control:DirectorController x:Name="DirectorController" Visibility="Hidden"/>
        <Control:MasterController x:Name="MasterController" Visibility="Hidden"/>
    </Grid>
    
</Window>
